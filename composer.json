{
    "name": "php-panel/laravel-admin-demo",
    "description": "The Laravel Framework.",
    "keywords": [
        "admin",
        "laravel-admin",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "php-panel/laravel-admin": "^3.0",
        "laravel/tinker": "~2.10",
        "intervention/image": "^3.11",
        "php-panel/ladmin-ext-code-mirror": "dev-master",
        "php-panel/ladmin-ext-api-tester": "^1.0.2",
        "php-panel/ladmin-ext-backup": "dev-master",
        "php-panel/ladmin-ext-chartjs": "dev-master",
        "php-panel/ladmin-ext-clike-editor": "dev-master",
        "php-panel/ladmin-ext-config": "dev-master",
        "php-panel/ladmin-ext-grid-lightbox": "dev-master",
        "php-panel/ladmin-ext-grid-sortable": "dev-master",
        "php-panel/ladmin-ext-helpers": "dev-master",
        "php-panel/ladmin-ext-js-editor": "dev-master",
        "php-panel/ladmin-ext-log-viewer": "dev-master",
        "php-panel/ladmin-ext-media-manager": "dev-master",
        "php-panel/ladmin-ext-php-editor": "dev-master",
        "php-panel/ladmin-ext-phpinfo": "dev-master",
        "php-panel/ladmin-ext-python-editor": "dev-master",
        "php-panel/ladmin-ext-reporter": "dev-master",
        "php-panel/ladmin-ext-scheduling": "dev-master",
        "php-panel/ladmin-ext-simplemde": "dev-master",
        "php-panel/ladmin-ext-summernote": "dev-master",
        "php-panel/ladmin-ext-wangeditor": "^3.0",
        "maatwebsite/excel": "~3.1",
        "matthiasmullie/minify": "^1.3",
        "spatie/eloquent-sortable": "^4.5",
        "spatie/laravel-backup": "^9.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "mockery/mockery": "1.6.*",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}